{
  "name": "3sq.re",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/threesquared/3sq.re.git"
  },
  "scripts": {
    "postinstall": "webpack -p && typings install",
    "watch": "webpack --watch",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build:prod": "webpack --progress -p",
    "server": "nodemon dist/server/index.js",
    "start": "npm run server",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/index.js",
    "sass": "node-sass src/assets/scss/style.scss --output src/assets/css --source-map true --output-style compressed",
    "sass:w": "npm run sass -- --watch=\"scss\" -r"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "angular2-universal": "~0.104.5",
    "body-parser": "^1.15.1",
    "bootstrap-sass": "^3.3.6",
    "express": "^4.14.0",
    "methods": "~1.1.2",
    "ng2-disqus": "~2.0.0",
    "ng2-page-scroll": "^1.2.1",
    "parse5": "^1.5.0",
    "preboot": "^2.1.2",
    "rxjs": "5.0.0-beta.6"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "cookie-parser": "^1.4.3",
    "nodemon": "^1.9.2",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.9",
    "typings": "~1.0.5",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.13.0"
  }
}
