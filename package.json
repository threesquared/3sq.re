{
  "name": "3sq.re",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/threesquared/3sq.re.git"
  },
  "scripts": {
    "watch": "webpack --watch",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build:prod": "webpack --progress -p",
    "prestart": "npm run build",
    "server": "nodemon dist/server/index.js",
    "debug:server": "node-nightly --inspect --debug-brk dist/server/index.js",
    "start": "npm run server",
    "debug:start": "npm run build && npm run debug:server",
    "predebug": "npm run build",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug": "node --debug-brk dist/server/index.js",
    "sass": "node-sass src/assets/scss/style.scss --output src/assets/css --source-map true --output-style compressed",
    "sass:w": "npm run sass -- --watch=\"scss\" -r",
    "package": "cp dist/lambda/index.js index.js && zip -q -r lambda.zip index.js node_modules src && rm index.js",
    "deploy:client": "AWS_DEFAULT_PROFILE=personal aws s3 cp ./dist/client/index.js s3://3sq.re/index.js --region eu-west-1 --acl public-read",
    "deploy:assets": "AWS_DEFAULT_PROFILE=personal aws s3 cp ./src/assets s3://3sq.re/assets --recursive --region eu-west-1 --acl public-read",
    "deploy": "npm run build:prod && npm run package && npm run deploy:client && npm run deploy:assets && npm run lambda:update && rm lambda.zip",
    "lambda:update": "AWS_DEFAULT_PROFILE=personal aws lambda update-function-code --function-name 3sq --region eu-west-1 --zip-file fileb://lambda.zip",
    "test": "karma start"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-express-engine": "~2.0.11",
    "angular2-highlight-js": "^3.0.0",
    "angular2-platform-node": "~2.0.11",
    "angular2-universal": "~2.0.11",
    "angular2-universal-polyfills": "~2.0.11",
    "aws-serverless-express": "^1.1.1",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.6",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "intl": "^1.2.5",
    "methods": "~1.1.2",
    "ng2-disqus": "^3.0.0",
    "ng2-page-scroll": "^3.1.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@angularclass/resolve-angular-routes": "^1.0.9",
    "@types/body-parser": "0.0.29",
    "@types/compression": "0.0.29",
    "@types/cookie-parser": "^1.3.29",
    "@types/ejs": "^2.3.32",
    "@types/express": "^4.0.32",
    "@types/express-serve-static-core": "^4.0.33",
    "@types/hammerjs": "^2.0.32",
    "@types/jasmine": "^2.5.35",
    "@types/mime": "0.0.28",
    "@types/node": "^6.0.38",
    "@types/serve-static": "^1.7.27",
    "@types/source-map": "^0.1.28",
    "@types/webpack": "^1.12.35",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "cookie-parser": "^1.4.3",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.10.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "github:gdi2290/string-replace-loader",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "tslint-microsoft-contrib": "^2.0.12",
    "typescript": "2.0.0",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.13.0"
  }
}
